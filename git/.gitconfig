[color]
  ui = true
[core]
  autocrlf = false
  editor = nvim
  eol = lf
  excludesfile = ~/.gitignore
  filemode = false
  pager = delta --plus-color="#012800" --minus-color="#340001" --theme='base16'
	commitGraph = true
[github]
  user = gajewsky
[interactive]
  diffFilter = delta --color-only
[pull]
  ff = only
[link]
  path= $HOME/.gitconfig-fragile
